<script>
    Morris.Line({
      element: 'line',
      data: [
      <% metrics_history(params[:id], 'page_posts_impressions', 60).each do |k,v|%>
        { date: '<%= raw k%>' , value: <%= v %>},
      <% end %>
      ],
      xkey: 'date',
      ykeys: ['value'],
      labels: ['Impressions'],
      parseTime: false,
      lineColors: ['#FF3D0D']
    });
  </script>